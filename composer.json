{
  "name": "noone-silent/phalcon-console",
  "description": "Phalcon console framework for easier console command management and overview.",
  "minimum-stability": "stable",
  "keywords": [
    "phalcon",
    "console",
    "cli",
    "command",
    "tasks"
  ],
  "license": "GPL-3.0-or-later",
  "homepage": "https://github.com/noone-silent/phalcon-console",
  "authors": [
    {
      "name": "n[oO]ne",
      "email": "lominum@protonmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Phalcon\\": "src/Phalcon/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Phalcon\\": "src/Phalcon/",
      "Unit\\": "tests/Unit/"
    }
  },
  "bin": [
    "bin/phalcon-console"
  ],
  "config": {
    "sort-packages": true
  },
  "extra": {
    "phalcon-console": {
      "bootstrap": "tests/test-bootstrap.php",
      "locations": [
        "tests/"
      ]
    }
  },
  "scripts": {
    "stan": "vendor/bin/phpstan analyse",
    "unit": "vendor/bin/phpunit"
  },
  "require-dev": {
    "phalcon/ide-stubs": "^5.9",
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5"
  },
  "require": {
    "php": ">= 8.0",
    "ext-phalcon": "^5.0",
    "php-parallel-lint/php-console-color": "^1.0"
  }
}
